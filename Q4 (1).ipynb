    "#for loop:\n",
    "list1 = []\n",
    "for i in range(1,101):\n",
    "    cube = i**3\n",
    "    if cube%4==0 or cube%5==0:\n",
    "        list1.append(i)\n",
    "print(list1)\n",
    "\n",
    "#while loop:\n",
    "list2 = []\n",
    "i=1\n",
    "while  i<101:\n",
    "    cube = i**3\n",
    "    if cube%4==0 or cube%5==0:\n",
    "        list2.append(i)\n",
    "    i=i+1\n",
    "print(list2)"
